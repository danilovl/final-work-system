{
    "name": "symfony/framework-standard-edition",
    "license": "MIT",
    "type": "project",
    "description": "The \"Symfony Standard Edition\" distribution",
    "autoload": {
        "psr-4": { "": "src/" },
        "classmap": [ "app/AppKernel.php", "app/AppCache.php" ]
    },
    "autoload-dev": {
        "psr-4": { "Tests\\": "tests/" },
        "files": [ "vendor/symfony/symfony/src/Symfony/Component/VarDumper/Resources/functions/dump.php" ]
    },
    "require": {
        "php": ">=7.1.0",
        "doctrine/cache": "^1.8",
        "doctrine/doctrine-bundle": "^1.10.2",
        "doctrine/doctrine-cache-bundle": "^1.3.5",
        "doctrine/orm": "^2.6.4",
        "egeloen/ckeditor-bundle": "5.0.3",
        "erusev/parsedown": "1.6",
        "ezyang/htmlpurifier": "^4.9",
        "fabpot/goutte": "^3.2",
        "friendsofsymfony/user-bundle": "^2.1.1",
        "gedmo/doctrine-extensions": "^2.4",
        "incenteev/composer-parameter-handler": "^2.0",
        "knplabs/knp-markdown-bundle": "^1.7.1",
        "knplabs/knp-paginator-bundle": "^2.7.2",
        "leafo/scssphp": "^0.6.7",
        "nochso/html-compress-twig": "^2.0",
        "overblog/graphiql-bundle": "^0.1.2",
        "overblog/graphql-bundle": "^0.12.3",
        "patchwork/jsqueeze": "^2.0",
        "phpspec/phpspec": "^4.3.4",
        "roukmoute/hashids-bundle": "^1.4",
        "sensio/distribution-bundle": "^5.0.24",
        "sensio/framework-extra-bundle": "^3.0.2",
        "snc/redis-bundle": "^3.1.1",
        "sonata-project/admin-bundle": "^3.54.1",
        "sonata-project/doctrine-orm-admin-bundle": "^3.9.0",
        "sonata-project/easy-extends-bundle": "^2.5.0",
        "sonata-project/formatter-bundle": "^3.4.1",
        "sonata-project/user-bundle": "^4.5.0",
        "symfony/assetic-bundle": "^2.8",
        "symfony/monolog-bundle": "^3.3.1",
        "symfony/polyfill-apcu": "^1.12.0",
        "symfony/swiftmailer-bundle": "^2.6.3",
        "symfony/symfony": "^3.4.35",
        "twig/twig": "^1.28 || ^2.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5.16",
        "sensio/generator-bundle": "^3.0",
        "symfony/phpunit-bridge": "^4.3.4"
    },
    "scripts": {
        "symfony-scripts": [
            "Incenteev\\ParameterHandler\\ScriptHandler::buildParameters",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::buildBootstrap",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::clearCache",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installAssets",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::installRequirementsFile",
            "Sensio\\Bundle\\DistributionBundle\\Composer\\ScriptHandler::prepareDeploymentTarget"
        ],
        "post-install-cmd": [
            "@symfony-scripts"
        ],
        "post-update-cmd": [
            "@symfony-scripts"
        ]
    },
    "config": {
        "platform": {
            "php": "7.1.4"
        },
        "sort-packages": true
    },
    "extra": {
        "symfony-app-dir": "app",
        "symfony-bin-dir": "bin",
        "symfony-var-dir": "var",
        "symfony-web-dir": "web",
        "symfony-tests-dir": "tests",
        "symfony-assets-install": "relative",
        "incenteev-parameters": {
            "file": "app/config/parameters.yml"
        },
        "branch-alias": {
            "dev-master": "3.2-dev"
        }
    }
}
