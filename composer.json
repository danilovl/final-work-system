{
    "name": "danilovl/final-work-system",
    "description": "Thesis management system based on Symfony",
    "type": "project",
    "license": "proprietary",
    "repositories": [
        {
            "type": "path",
            "url": "packages/friendsofsymfony/elastica-bundle",
            "options": {
                "symlink": false
            }
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": "^8.5",
        "ext-amqp": "*",
        "ext-ctype": "*",
        "ext-gd": "*",
        "ext-iconv": "*",
        "ext-mbstring": "*",
        "ext-opentelemetry": "*",
        "ext-pdo": "*",
        "ext-redis": "*",
        "ext-sockets": "*",
        "ext-sodium": "*",
        "api-platform/core": "^4.0.11",
        "api-platform/doctrine-orm": "*",
        "api-platform/symfony": "*",
        "aws/aws-sdk-php": "^3.279",
        "aws/aws-sdk-php-symfony": "^2.7",
        "composer/package-versions-deprecated": "1.11.99.3",
        "danilovl/async-bundle": "^1.0.0",
        "danilovl/cache-response-bundle": "^1.0.0",
        "danilovl/doctrine-entity-dto-bundle": "^1.0.0",
        "danilovl/hashids-bundle": "^2.0.0",
        "danilovl/object-dto-mapper": "^1.0.0",
        "danilovl/object-to-array-transform-bundle": "^4.0.0",
        "danilovl/parameter-bundle": "^5.0.1",
        "danilovl/permission-middleware-bundle": "^3.0.0",
        "danilovl/render-service-twig-extension-bundle": "^5.0.0",
        "danilovl/select-autocompleter-bundle": "^4.0.1",
        "danilovl/symfony-console-input-validation": "^1.0.0",
        "danilovl/translator-bundle": "^1.0.0",
        "doctrine/doctrine-bundle": "^3.2",
        "doctrine/doctrine-migrations-bundle": "^3.2",
        "doctrine/orm": "^3.6",
        "easycorp/easyadmin-bundle": "^4.27",
        "friendsofsymfony/elastica-bundle": "^8.0.0",
        "gedmo/doctrine-extensions": "^3.14",
        "hashids/hashids": "^5.0.0",
        "knplabs/knp-paginator-bundle": "^6.0.1",
        "nelmio/api-doc-bundle": "^5.5",
        "nelmio/cors-bundle": "^2.2",
        "open-telemetry/api": "dev-main#04c85a1",
        "open-telemetry/exporter-otlp": "dev-main#9b6de12",
        "open-telemetry/gen-otlp-protobuf": "dev-main#66c3b98",
        "open-telemetry/sdk": "dev-main#f0457fe",
        "open-telemetry/sem-conv": "^1.25",
        "phpdocumentor/reflection-docblock": "^5.3",
        "phpdocumentor/type-resolver": "^1.10.0",
        "phpstan/phpdoc-parser": "^1.25",
        "predis/predis": "^2.3",
        "symfony/amqp-messenger": "8.0.*",
        "symfony/asset": "8.0.*",
        "symfony/asset-mapper": "8.0.*",
        "symfony/console": "8.0.*",
        "symfony/doctrine-messenger": "8.0.*",
        "symfony/dotenv": "8.0.*",
        "symfony/expression-language": "8.0.*",
        "symfony/flex": "^2.2.4",
        "symfony/form": "8.0.*",
        "symfony/framework-bundle": "8.0.*",
        "symfony/http-client": "8.0.*",
        "symfony/http-kernel": "8.0.*",
        "symfony/intl": "8.0.*",
        "symfony/mailer": "8.0.*",
        "symfony/mercure-bundle": "^0.3.7",
        "symfony/messenger": "8.0.*",
        "symfony/mime": "8.0.*",
        "symfony/monolog-bundle": "^4.0",
        "symfony/notifier": "8.0.*",
        "symfony/process": "8.0.*",
        "symfony/property-access": "8.0.*",
        "symfony/property-info": "8.0.*",
        "symfony/runtime": "8.0.*",
        "symfony/security-bundle": "8.0.*",
        "symfony/serializer": "8.0.*",
        "symfony/string": "8.0.*",
        "symfony/translation": "8.0.*",
        "symfony/twig-bundle": "8.0.*",
        "symfony/ux-autocomplete": "^2.22",
        "symfony/ux-live-component": "^2.22",
        "symfony/validator": "8.0.*",
        "symfony/web-link": "8.0.*",
        "symfony/webpack-encore-bundle": "^2.1.1",
        "symfony/yaml": "8.0.*",
        "twig/extra-bundle": "^3.0",
        "twig/twig": "^3.0",
        "webmozart/assert": "^1.11",
        "webonyx/graphql-php": "^15.0.1"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.92.5",
        "phpstan/extension-installer": "^1.4.3",
        "phpstan/phpstan": "^2.1.33",
        "phpstan/phpstan-doctrine": "^2.0.12",
        "phpstan/phpstan-symfony": "^2.0.9",
        "phpunit/phpunit": "^12.5.6",
        "symfony/browser-kit": "8.0.*",
        "symfony/css-selector": "8.0.*",
        "symfony/debug-bundle": "8.0.*",
        "symfony/maker-bundle": "^1.65.1",
        "symfony/phpunit-bridge": "8.0.*",
        "symfony/stopwatch": "8.0.*",
        "symfony/web-profiler-bundle": "8.0.*"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": {
            "*": "dist"
        },
        "sort-packages": true,
        "allow-plugins": {
            "composer/package-versions-deprecated": true,
            "php-http/discovery": true,
            "phpstan/extension-installer": true,
            "symfony/flex": true,
            "symfony/runtime": true,
            "tbachert/spi": true
        }
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "App\\Tests\\": "tests/"
        }
    },
    "replace": {
        "paragonie/random_compat": "2.*",
        "symfony/polyfill-ctype": "*",
        "symfony/polyfill-iconv": "*",
        "symfony/polyfill-php72": "*",
        "symfony/polyfill-php71": "*",
        "symfony/polyfill-php70": "*",
        "symfony/polyfill-php56": "*"
    },
    "scripts": {
        "warm-up-container": "bash bin/warm-up-container.sh",
        "test-unit": "bash bin/phpunit.sh tests/Unit",
        "test-integration": [
            "@php bin/console cache:warmup --env=dev",
            "bash bin/phpunit.sh tests/Integration"
        ],
        "test-all": [
            "@php bin/console cache:warmup --env=dev",
            "bash bin/phpunit.sh '--testsuite tests-unit,tests-integration,tests-functional'"
        ],
        "test-web": [
            "@php bin/console cache:warmup --env=dev",
            "bash bin/web-test.sh"
        ],
        "cs-fixer": "vendor/bin/php-cs-fixer fix --allow-risky=yes",
        "cs-fixer-check": "vendor/bin/php-cs-fixer fix --dry-run --allow-risky=yes",
        "phpstan": "phpstan analyse -c phpstan.neon --no-progress",
        "phpstan-generate-baseline": "vendor/bin/phpstan analyse --configuration phpstan.neon --generate-baseline",
        "phpstan-clear-result-cache": "phpstan clear-result-cache",
        "psalm": "./vendor/bin/psalm --no-cache",
        "psalm-update-baseline": "./vendor/bin/psalm --no-cache --set-baseline=psalm-baseline.xml",
        "pre-push": "@phpunit",
        "auto-scripts": {
            "cache:clear": "symfony-cmd",
            "assets:install %PUBLIC_DIR%": "symfony-cmd",
            "importmap:install": "symfony-cmd"
        },
        "post-install-cmd": "@auto-scripts",
        "post-update-cmd": "@auto-scripts"
    },
    "conflict": {
        "symfony/symfony": "*"
    },
    "extra": {
        "symfony": {
            "allow-contrib": false,
            "require": "8.0.*"
        }
    },
    "platform": {
        "php": "^8.5"
    }
}
